
public class Methods { // Class declaration
     public static void main (String[] args) {
    	 sayHelloMontay("Montay Henson");
    	 sayHelloMontay("You are 24");
    	 Bye();
    	 
    	 int x = returnFive();
    	 
    	   int result = square (5);
    	 System.out.println("The value from square is " + result);
    	
     }
static int square(int x) {
	
	return x * x;
}
//end main class
     {
  
  }
     
     
     
     
//this method returns an int type with the value 5
     static int returnFive() {
     return 5*5*5*5*600;
     }
     
     //This method say's hello to whatever name is passed when called 
     static void sayHelloMontay(String name) {
    	 System.out.println("Hello," + name);
     }
     
     
     //This Method says bye
     
    static void Bye(){
    	System.out.println("Bye Bye!");
    }
     
}
